{
	"name": "bancho-box",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run build:data && vite dev",
		"build": "npm run build:data && vite build",
		"preview": "vite preview",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest run",
		"test": "npm run test:unit || true && npm run test:e2e",
		"test:e2e": "playwright test",
		"build:data": "tsx scripts/build-database.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.34.0",
		"@fontsource/fira-mono": "^5.2.6",
		"@lucide/lab": "^0.1.2",
		"@lucide/svelte": "^0.537.0",
		"@neoconfetti/svelte": "^2.2.2",
		"@playwright/test": "^1.55.0",
		"@skeletonlabs/skeleton": "^3.2.0",
		"@skeletonlabs/skeleton-svelte": "^1.5.1",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/kit": "^2.37.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"@types/node": "^24.3.0",
		"@vitest/browser": "^3.2.4",
		"bits-ui": "^2.9.4",
		"csv-parse": "^6.1.0",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"mdsvex": "^0.12.6",
		"playwright": "^1.55.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.6",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.12",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0",
		"vite": "^7.1.3",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^0.1.0",
		"wrangler": "^4.33.1",
		"zod": "^4.1.5"
	},
	"dependencies": {},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,mdx,css,scss,html,yml,yaml}": "prettier --write"
	},
	"trustedDependencies": [
		"@tailwindcss/oxide"
	]
}
